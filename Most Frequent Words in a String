#Problem 1: Most Frequent Words in a string

def freqword(st, k):
    counts = {}
    for i in range(len(st)-k+1):
        if st[i:i+k] not in counts:
            counts[st[i:i+k]] = 0
        counts[st[i:i+k]] += 1
    m = max(counts.values())
    out = []
    for kmer in counts:
        if counts[kmer] == m:
            out.append(kmer)
    return out

st = ''
k = 12
print (" ".join(freqword(st,k)))
