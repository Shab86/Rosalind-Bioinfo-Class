#Problem 1: Most Frequent Words in a string

def freqword( txt, k ):
    counts = {}
    for i in range(len(txt)-k+1):
        if txt[i:i+k] not in counts:
            counts[txt[i:i+k]] = 0
        counts[txt[i:i+k]] += 1
    m = max(counts.values())
    out = []
    for kmer in counts:
        if counts[kmer] == m:
            out.append(kmer)
    return out

txt = ''
k = 12
print (" ".join(freqword(txt,k)))
